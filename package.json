{
  "name": "react-portal-hint",
  "version": "0.5.1",
  "description": "Yet another implementation for Tooltip/Popover using React Portals.",
  "repository": "https://github.com/occar421/react-portal-hint",
  "author": "MasuqaT <occar@hotmail.co.jp>",
  "license": "MIT",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "rollup -c && node-sass ./src/ -o dist/",
    "style-watch": "node-sass ./src/ -o src/ && node-sass ./src/ -o src/ --watch",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "node-sass ./src/ -o src/ && build-storybook -c .storybook",
    "serve-static-storybook": "http-server ./storybook-static -p 8765",
    "test:unit": "jest",
    "test:e2e": "cypress run --env BASE_URL=http://localhost:8765"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@storybook/addon-info": "^5.0.3",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/react": "^4.1.1",
    "@types/jest": "^23.3.12",
    "@types/react": "~16.8",
    "@types/react-dom": "~16.8",
    "@types/react-is": "~16.7.0",
    "@types/storybook__addon-knobs": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "cypress": "^3.1.5",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react": "~16.8",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-dom": "~16.8",
    "react-draggable": "^3.1.1",
    "react-is": "~16.8",
    "react-testing-library": "^5.5.4",
    "rollup": "^1.0.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "wait-on": "^3.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "react-is": ">=16.8"
  },
  "dependencies": {
    "resize-observer-polyfill": "^1.5.1"
  },
  "keywords": [
    "react",
    "react-component",
    "portal",
    "hint",
    "tooltip",
    "popover"
  ]
}
